@article{ ISI:000300772500032,
Author = {Landguth, Erin L. and Muhlfeld, C. C. and Luikart, G.},
Title = {{CDFISH: an individual-based, spatially-explicit, landscape genetics
   simulator for aquatic species in complex riverscapes}},
Journal = {{CONSERVATION GENETICS RESOURCES}},
Year = {{2012}},
Volume = {{4}},
Number = {{1}},
Pages = {{133-136}},
Month = {{MAR}},
Abstract = {{We introduce Cost Distance FISHeries (CDFISH), a simulator of population
   genetics and connectivity in complex riverscapes for a wide range of
   environmental scenarios of aquatic organisms. The spatially-explicit
   program implements individual-based genetic modeling with Mendelian
   inheritance and k-allele mutation on a riverscape with resistance to
   movement. The program simulates individuals in subpopulations through
   time employing user-defined functions of individual migration,
   reproduction, mortality, and dispersal through straying on a continuous
   resistance surface.}},
