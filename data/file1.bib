@article{ ISI:000311399000015,
Author = {Sepulveda-Villet, Osvaldo J. and Stepien, Carol A.},
Title = {{Waterscape genetics of the yellow perch (Perca flavescens): patterns
   across large connected ecosystems and isolated relict populations}},
Journal = {{MOLECULAR ECOLOGY}},
